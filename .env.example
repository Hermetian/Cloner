# Cloner Environment Variables Template
# Copy this file to .env and fill in your actual values

# Voice Cloning API Keys
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
RESEMBLE_API_KEY=your_resemble_api_key_here

# Meeting Platform Credentials (if needed)
GOOGLE_MEET_EMAIL=your_email@gmail.com
GOOGLE_MEET_PASSWORD=your_password_here

# Optional: Zoom credentials
ZOOM_EMAIL=your_email@example.com
ZOOM_PASSWORD=your_password_here

# Project Settings
DEBUG=false
LOG_LEVEL=INFO
